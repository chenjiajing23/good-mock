{
    "name": "good-mock",
    "version": "0.0.1",
    "description": "好用的mock数据模板",
    "main": "dist/index.js",
    "module": "dist/index-esm-min.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "ISC",
    "author": "chenjiajing",
    "private": false,
    "homepage": "https://github.com/chenjiajing23/good-mock",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenjiajing23/good-mock.git"
    },
    "keywords": [
        "mock",
        "typescript"
    ],
    "scripts": {
        "dev": "rollup -c --watch",
        "build": "rm -rf dist/*  &&  NODE_ENV=production rollup  -c",
        "build:win": "rm -rf dist/*  && cross-env NODE_ENV=production rollup  -c",
        "reload": "livereload 'src/'",
        "eslint": "eslint src --ext .tsx,.ts --color --fix",
        "test": "parcel test/index.html --out-dir test/dist",
        "watch": "npm-run-all --parallel reload dev test",
        "gc": "git-cz"
    },
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.10.3",
        "@babel/preset-env": "^7.10.4",
        "@commitlint/config-conventional": "^9.0.1",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-commonjs": "^13.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^8.1.0",
        "@types/node": "^14.0.14",
        "commitizen": "^4.1.2",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.2",
        "eslint": "^7.3.1",
        "livereload": "^0.9.1",
        "npm-run-all": "^4.1.5",
        "parcel": "^1.12.4",
        "prettier": "^2.0.5",
        "rollup": "^2.18.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-sourcemaps": "^0.6.2",
        "rollup-plugin-terser": "^6.1.0",
        "rollup-plugin-typescript2": "^0.27.1",
        "rollup-watch": "^4.3.1",
        "tslib": "^2.0.0",
        "typescript": "^3.9.6"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run eslint && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx,css,less,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
